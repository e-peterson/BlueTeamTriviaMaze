<Window x:Class="BlueTeamTriviaMaze.DeadEndWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Dead End" Height="300" Width="300" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" Icon="Resources/TriviaMazeIcon.png">
    <Window.Background>
        <ImageBrush ImageSource="Resources/QuestionWindowBackground.png"/>
    </Window.Background>
    <Grid>
        <Label x:Name="lblDeadEnd" Content="Dead End" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="272" Foreground="#FFBF0C0C" FontFamily="/BlueTeamTriviaMaze;component/Resources/#Triforce" FontSize="48" HorizontalContentAlignment="Center" OpacityMask="Black">
            <Label.Effect>
                <DropShadowEffect BlurRadius="2" Color="White" Direction="0" ShadowDepth="2"/>
            </Label.Effect>
        </Label>
        <TextBlock x:Name="txblkDeadEnd" HorizontalAlignment="Left" TextWrapping="Wrap" VerticalAlignment="Top" Margin="10,76,0,0" Height="184" Width="272" FontFamily="/BlueTeamTriviaMaze;component/Resources/#Triforce" FontSize="16" Foreground="White" TextOptions.TextFormattingMode="Display"/>

    </Grid>
</Window>
